nerNameWithBackupItemName, BillingEntityUniqueId = BackupItemUniqueId;\r\n};\r\nlet ProtectedInstanceMetric = ( ) \r\n{ CombinedTable | union isfuzzy = true \r\n(LatestProtectedInstanceTableFromProtectedContainerUniqueId()),\r\n(LatestProtectedInstanceTableFromBackupItemUniqueId())\r\n| where BMSTypeWithBackupItemType in ({BMSTypeWithBackupItemType}) or '*' in ({BMSTypeWithBackupItemType})\r\n| where BackupItemProtectionState in (@\"{BackupItemProtectionState}\") or '*' in (@\"{BackupItemProtectionState}\")\r\n| extend PolicyLink = strcat(ResourceId, \"/backupPolicies/\", PolicyName)\r\n| where PolicyLink == (@\"{SelectedPolicyLink}\") or '*' in (@\"{SelectedPolicyLink}\")\r\n| summarize sum(BackupItemFrontEndSize), sum(StorageConsumedInMB) by BillingEntityUniqueId, BillingEntityName, BillingEntityType, BackupManagementType, ProtectedInstanceCount, TimeRangeEndDay\r\n| project-away BillingEntityUniqueId\r\n};\r\nProtectedInstanceMetric\r\n| summarize sum(ProtectedInstanceCount) by  TimeRangeEndDay",
